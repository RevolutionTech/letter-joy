    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-plugin-react": "^7.31.8",
    "nodemon": "^2.0.20",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
