    "@fullstory/browser": "^1.6.2",
    "@hello-pangea/dnd": "^16.0.0",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.7",
    "@sentry/node": "^7.14.0",
    "@sentry/react": "^7.5.1",
    "@sentry/tracing": "^7.5.1",
    "boardgame.io": "^0.49.12",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
