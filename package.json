{
  "name": "letter-joy",
  "version": "0.1.0",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf .cache dist",
    "client:watch": "parcel watch src/index.html -d dist/client",
    "client:build": "parcel build src/index.html -d dist/client",
    "server:start": "nodemon",
    "server:build": "tsc -p tsconfig.server.json",
    "typecheck": "tsc --noemit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint --fix . --ext .ts,.tsx"
  },
  "browserslist": "defaults and supports async-functions",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@babel/core": "^7.18.6",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.2",
    "@mui/material": "^5.8.2",
    "@react-forked/dnd": "^14.0.2",
    "@sentry/node": "^7.0.0",
    "boardgame.io": "^0.49.12",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "react-textfit": "^1.1.1"
  },
  "devDependencies": {
    "@types/koa-static": "^4.0.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.40",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.17.0",
    "eslint-plugin-react": "^7.30.0",
    "nodemon": "^2.0.16",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  }
}
