    "boardgame.io": "^0.49.10",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-full-screen": "^1.1.0",
    "react-textfit": "^1.1.1"
